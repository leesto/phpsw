[program:redis-commander]
command = redis-commander --redis-host db.phpsw.uk --http-auth-username {{ redis.commander.http_auth_username }} --http-auth-password {{ redis.commander.http_auth_password }}
user = root
autostart = true
autorestart = true
stdout_logfile = /var/log/supervisor/redis-commander.log
stderr_logfile = /var/log/supervisor/redis-commander_err.log
